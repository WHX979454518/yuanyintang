<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/dimen_10"
    android:background="@color/white"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <EditText
            android:id="@+id/et_work_type"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@color/white"
            android:hint="输入工种类型 唱见/作词/作曲/策划"
            android:padding="@dimen/dimen_15"
            android:textColorHint="#9da6a4"
            android:textColor="@color/text_color"
            android:textSize="@dimen/size_14"

            />

        <ImageView
            android:id="@+id/img_del_partner_item"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:padding="@dimen/dimen_15"
            android:src="@drawable/icon_add_delect" />


    </LinearLayout>

    <include layout="@layout/view_devider" />

    <com.mxkj.yuanyintang.mainui.myself.my_release.view.AddPartnerViewGroup
        android:id="@+id/partner_view_group"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dimen_10"
        app:child_layout="@layout/child_partner_grop"
        app:child_margin_horizontal="@dimen/dimen_5"
        app:child_margin_vertical="@dimen/dimen_5"


        />

    <LinearLayout
        android:background="@color/linerColor"
        android:layout_width="match_parent"
        android:layout_height="10px"></LinearLayout>


</LinearLayout>