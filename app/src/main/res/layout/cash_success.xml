<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/ll_cash_success"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:visibility="gone"
    android:orientation="vertical">

    <ImageView
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dimen_65"
        android:src="@drawable/cash_success" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_37"
        android:gravity="center_horizontal"
        android:text="提现申请已提交"
        android:textColor="#1a1717"
        android:textSize="@dimen/size_16"

        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="@dimen/dimen_30"
        android:layout_marginLeft="@dimen/dimen_63"
        android:layout_marginRight="@dimen/dimen_63"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:text="提现银行 "
            android:textColor="#9da6a4"
            android:textSize="@dimen/size_14"

            />

        <TextView
            android:id="@+id/tv_bank_desc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_20"
            android:layout_marginRight="@dimen/dimen_40"
            android:gravity="center_horizontal"
            android:text=""
            android:textColor="#1a1717"
            android:textSize="@dimen/size_14"

            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_15"
        android:layout_marginLeft="@dimen/dimen_63"
        android:layout_marginRight="@dimen/dimen_63"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:text="提现金额"
            android:textColor="#9da6a4"
            android:textSize="@dimen/size_14"

            />

        <TextView
            android:id="@+id/money_desc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_20"
            android:layout_marginRight="@dimen/dimen_40"
            android:gravity="center_horizontal"
            android:text="-"
            android:textColor="#1a1717"
            android:textSize="@dimen/size_14"

            />

    </LinearLayout>

    <TextView
        android:id="@+id/msg_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimen_47"
        android:layout_marginRight="@dimen/dimen_47"
        android:layout_marginTop="@dimen/dimen_20"
        android:layout_gravity="center"
        android:text=""
        android:textColor="#616665"
        android:textSize="@dimen/size_14"

        />

    <TextView
        android:id="@+id/tv_cash_history"
        android:layout_width="300dp"
        android:layout_height="@dimen/dimen_40"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dimen_30"
        android:background="@drawable/shape_button_red_incode"
        android:gravity="center"
        android:text="查看提现记录"
        android:textColor="@color/white"
        android:textSize="@dimen/size_15" />


    <TextView
        android:id="@+id/back_botom"
        android:padding="@dimen/dimen_20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:textSize="@dimen/size_14"
        android:text="返回"
        android:textColor="@color/base_red"

        />


</LinearLayout>